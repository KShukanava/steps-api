{
  "name": "@qavajs/steps-api",
  "version": "0.16.0",
  "description": "api steps for @qavajs project",
  "main": "./index.js",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "lint:write": "npm run pretty && eslint ./ --fix",
    "pretty": "prettier --write ./",
    "test:e2e": "ts-node --project tsconfig.json node_modules/@qavajs/cli/bin/qavajs.js run --config test-e2e/api.ts",
    "debug:e2e": "ts-node --project tsconfig.json node_modules/@qavajs/cli/bin/qavajs.js run --config test-e2e/api.ts --tags @debug"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:write"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qavajs/steps-api.git"
  },
  "keywords": [
    "QA",
    "Test Automation"
  ],
  "authors": [
    "Alexandr Galichenko",
    "Kiryl Bahdanets",
    "Dzmitry Prakapuk",
    "Alexandr Legchilov"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qavajs/steps-api/issues"
  },
  "homepage": "https://github.com/qavajs/steps-api#readme",
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@cucumber/cucumber": "^9.1.2",
    "@qavajs/cli": "^0.26.0",
    "@qavajs/console-formatter": "^0.2.1",
    "@qavajs/html-formatter": "^0.0.11",
    "@qavajs/memory": "^1.5.0",
    "@qavajs/xunit-formatter": "^0.0.4",
    "@types/chai": "^4.3.5",
    "@types/jest": "^29.5.2",
    "@types/node": "^18.15.11",
    "@types/node-fetch": "^2.6.4",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "8.42.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-cucumber": "^2.0.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-wdio": "^8.8.7",
    "husky": "^8.0.3",
    "nock": "^13.3.1",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "vitest": "^0.32.4"
  },
  "dependencies": {
    "@qavajs/api-service": "^0.0.8",
    "@qavajs/validation": "^0.0.5",
    "form-data": "^4.0.0",
    "node-fetch": "^2.6.7"
  }
}
